@page
@model BoardGames.GameNightsModel
@{
    ViewData["Title"] = "GameNights";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>GameNights</h1>

<div class="row">
    <div class="col-md-3">Date</div> @*date of game night*@
    <div class="col-md-3">Type</div> @*type: open - tournament*@
    <div class="col-md-1">Attendees</div> @*number of attendees - tournys have sign ups*@
    <div class="col-md-1">Openings</div> @*How many people can join*@
    <div class="col-md-3">Join</div> @*join if spots are available.*@
</div>

@*Add for loop here for each game night*@